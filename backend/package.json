{
  "name": "personalfinancetracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "nodemon --exec node --inspect -r ts-node/register -r dotenv/config src/index.ts",
    "dev": "nodemon",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write .",
    "build": "rimraf ./dist && tsc && tsc-alias -p tsconfig.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "run_db_migration": "npx typeorm-ts-node-commonjs migration:run --dataSource ./src/config/dbSource.ts",
    "revert_db_migration": "npx typeorm-ts-node-commonjs migration:revert --dataSource ./src/config/dbSource.ts",
    "run_data_migration": "npx typeorm-ts-node-commonjs migration:run --dataSource ./src/config/dataSource.ts",
    "revert_data_migration": "npx typeorm-ts-node-commonjs migration:revert --dataSource ./src/config/dataSource.ts",
    "create_migration": "npx ts-node ./src/scripts/create-migration.ts",
    "create_entity": "npx typeorm entity:create src/entity/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-auth-library": "^9.14.2",
    "pg": "^8.13.1",
    "pug": "^3.0.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "save-dev": "^0.0.1-security",
    "typeorm": "^0.3.20",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.7",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^15.11.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
